        </requires>
        <extension point="xbmc.addon.repository" name="Atto Byte">
            <info compressed="false">https://raw.githubusercontent.com/williamluke4/kodi-repository.william.luke.org/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/williamluke4/kodi-repository.william.luke.org/master/_repo/addons.xml.md5</checksum>
